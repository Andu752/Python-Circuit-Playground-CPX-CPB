def main():
    x=20
    y=35
    z=x*y
    print(z)
    e=z/10
    q=e*3+69
    big_Number= x + y * z + e + q
    print(big_Number)
    if(big_Number == 24869):
        print('Very Big')
    else:
        print('Not huge')

main()
